package ipractice;

public class longestpalindrome {
	
	static void longpali(String s){
		
		char []chr= s.toCharArray();
		for(int i=0;i<chr.length;i++){
			if(chr[i]==' ')
			{chr[i]=chr[i++];
			}
		System.out.print(chr[i]);
		}
	}
	
	public static void main(String args[])
	{
		String str= new String("Gabbar rocks");
		longpali(str);
		
	}

}
